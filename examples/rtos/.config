
#
# AIVA ai31xx SDK Kconfig
#
# CONFIG_DEBUG is not set

#
# Chip seclect
#
# CONFIG_USE_FPGA is not set
CONFIG_CHIP_AI1101A=y
# CONFIG_CHIP_AI1102A is not set
# CONFIG_CHIP_AI1102B is not set
# CONFIG_CHIP_AI1102C is not set
# CONFIG_CHIP_AI1103A is not set
# CONFIG_CHIP_AI1103B is not set
# CONFIG_CHIP_AI1103C is not set
# end of Chip seclect

#
# Boot options
#
CONFIG_BOOT_DELAY_SECS=2
# CONFIG_EXECUTE_ON_DDR is not set
CONFIG_EXECUTE_ON_ROM=y
CONFIG_XIP_BOOT_ADDR=0x30010000
# end of Boot options

#
# Memory map configure
#
CONFIG_SRAM_CODE_SIZE_KB=80

#
# DMA_MEM_SIZE_KB: max 1024KB, 4KB page aligned
#
CONFIG_DMA_MEM_SIZE_KB=128

#
# DMA_MEM_SIZE_KB: max DMA_MEM_SIZE_KB, 4KB page aligned
#
CONFIG_DMA_HEAP_SIZE_KB=56

#
# The memory block which is smaller than SMALL_MEM_SIZE_BYTE will be allocated from small memory pool
#
CONFIG_SMALL_MEM_SIZE_BYTE=1060
CONFIG_SMALL_MEM_POOL_SIZE_BYTE=413696
# CONFIG_SRAM_RELOCATE_CODE is not set
# end of Memory map configure

#
# AIVA ai11xx SDK component Kconfig
#

#
# Device drivers
#

#
# Audio drivers
#
# CONFIG_AUDIO_ES8374 is not set
# CONFIG_AUDIO_ES8311 is not set
# CONFIG_AUDIO_ES8388 is not set

#
# Fingerprint drivers
#
# CONFIG_FPC1150 is not set

#
# Flash drivers
#

#
# NAND flash
#
# CONFIG_GD5FXX is not set
# CONFIG_MX35LFXXE4 is not set
# CONFIG_W25NXX is not set

#
# NAND flash
#
# CONFIG_NAND_FLASH_STD is not set

#
# NOR flash
